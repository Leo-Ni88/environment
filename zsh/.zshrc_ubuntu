# use cross compile
cross_compile() {
  export ARCH=arm
  export CROSS_COMPILE=arm-linux-gnueabi-
  echo "cross compile on"
}

# unuse cross compile
nocross_compile() {
  unset ARCH
  unset CROSS_COMPILE
  echo "cross compile off"
}

