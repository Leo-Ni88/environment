# function
function picocom_start()
{
  sudo picocom -b $1  --raise-dtr --lower-rts --imap lfcrlf $2 -g /home/leo/workspace/log/picocom/`basename $2`_`date "+%Y_%m_%d_%H_%M_%S"`.log
}

alias bathelp='bat --plain --language=help'
help() {
    "$@" --help 2>&1 | bathelp
}
